        "brdev/cookie": "^1.0",
        "jenssegers/blade": "^1.4",
        "coffeecode/optimizer": "^2.1",
        "coffeecode/datalayer": "^2.0",
        "filp/whoops": "^2.15",
        "symfony/var-dumper": "^6.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
